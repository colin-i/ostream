
echo "test/xver? y? else git show master:verion.txt" && \
read -n1 -t $((60*60*24)) ans && \
if [ "${ans}" = "y" ]; then
	echo xver && \
	~/test/xver ./version.txt
else
	echo git show && \
	git show master:version.txt > version.txt
	cat version.txt
fi && \
~/test/debinit2b "${1}" && \
~/test/deb && \
~/test/publp && \
~/test/publp2
